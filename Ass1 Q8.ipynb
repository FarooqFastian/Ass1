   ],
   "source": [
    "vowel = {'A','E','I','O','U','a','e','i','o','u'}\n",
    "char = input(\"Enter a character: \")\n",
    "count=0\n",
    "for n in vowel:\n",
    "    if(n == char):\n",
    "        count=1\n",
    "        break\n",
    "\n",
    "if(count==1):\n",
    "    print(\"Letter {0} is Vowel\".format(char))\n",
    "else:\n",
    "    print(\"Letter {0} is not Vowel\".format(char))\n",
    "        "
